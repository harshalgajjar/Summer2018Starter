113957b1e652"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"cone", "[", 
   RowBox[{"\[Theta]_", ",", "z_"}], "]"}], ":=", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{
     RowBox[{"Sqrt", "[", 
      RowBox[{"(", 
       SuperscriptBox["z", "2"], ")"}], "]"}], "*", 
     RowBox[{"Cos", "[", "\[Theta]", "]"}]}], ",", 
    RowBox[{
     RowBox[{"Sqrt", "[", 
      RowBox[{"(", 
       SuperscriptBox["z", "2"], ")"}], "]"}], "*", 
     RowBox[{"Sin", "[", "\[Theta]", "]"}]}], ",", "z"}], "}"}]}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"cylinder", "[", 
    RowBox[{"\[Theta]_", ",", "z_"}], "]"}], ":=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"Cos", "[", "\[Theta]", "]"}], ",", 
     RowBox[{"Sin", "[", "\[Theta]", "]"}], ",", "z"}], "}"}]}], "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", 
    RowBox[{"\[Theta]_", ",", "z_"}], "]"}], ":=", 
   RowBox[{"cone", "[", 
    RowBox[{"\[Theta]", ",", "z"}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"points", "=", 
   RowBox[{"Flatten", "[", 
    RowBox[{
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"f", "[", 
        RowBox[{"\[Theta]", ",", "z"}], "]"}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"\[Theta]", ",", 
         RowBox[{"-", "Pi"}], ",", "Pi", ",", "0.05"}], "}"}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"z", ",", "0", ",", 
         RowBox[{"+", "0.5"}], ",", "0.05"}], "}"}]}], "]"}], ",", " ", "1"}],
     "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"\"\<Points: \>\"", 
   RowBox[{"Short", "[", 
    RowBox[{"points", ",", "5"}], "]"}]}], "\n"}], "\n", 
 RowBox[{"ListPointPlot3D", "[", 
  RowBox[{"points", ",", " ", 
   RowBox[{"PlotRange", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"-", "1"}], ",", "1"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"-", "1"}], ",", "1"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"-", "1"}], ",", "1"}], "}"}]}], "}"}]}], ",", " ", 
   RowBox[{"BoxRatios", "\[Rule]", "1"}], ",", " ", 
   RowBox[{"AxesLabel", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"x", ",", "y", ",", "z"}], "}"}]}]}], "]"}]}], "Code",
