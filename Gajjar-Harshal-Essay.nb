Cell[BoxData[{
 RowBox[{
  RowBox[{"\[Alpha]", " ", "=", " ", "0"}], ";"}], "\n", 
 RowBox[{"{", 
  RowBox[{
   RowBox[{"\"\<\[Alpha] \>\"", 
    RowBox[{"Slider", "[", 
     RowBox[{
      RowBox[{"Dynamic", "[", "\[Alpha]", "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"0", ",", 
        RowBox[{"2", "*", "Pi"}], ",", "0.2"}], "}"}]}], "]"}]}], ",", 
   RowBox[{"Dynamic", "[", "\[Alpha]", "]"}]}], "}"}]}], "Code",
